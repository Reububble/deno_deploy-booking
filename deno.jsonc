{
  "fmt": {
    "lineWidth": 160
  },
  "imports": {
    "esbuild/": "https://deno.land/x/esbuild@v0.21.4/",
    "esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@^0.10.3",
    "@deno:std/": "https://deno.land/std@0.224.0/"
  },
  "compilerOptions": {
    "lib": [
      "esnext",
      "dom"
    ]
  },
  "tasks": {
    "build": "deno run --allow-env=DENO_REGISTRY_URL --allow-net=deno.land --allow-read=./,imports.ts --allow-write=./dist/,./src/imports.ts build.ts"
  }
}
